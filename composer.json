{
  "name": "gboquizo/taka",
  "type": "project",
  "description": "Multi-account manager",
  "keywords": [
    "laravel",
    "account",
    "dinero"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "akaunting/laravel-money": "^5.1",
    "awcodes/filament-badgeable-column": "^2.0",
    "archtechx/enums": "^0.3.2",
    "bavix/laravel-wallet": "^10.0",
    "blade-ui-kit/blade-icons": "^1.5",
    "cviebrock/eloquent-sluggable": "^10.0",
    "filament/filament": "^3.0",
    "filament/notifications": "^3.0",
    "guava/filament-icon-picker": "^2.0",
    "guzzlehttp/guzzle": "^7.2",
    "jeffgreco13/filament-breezy": "^v2.1.1",
    "laravel/framework": "^10.10",
    "laravel/helpers": "^1.6",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "leandrocfe/filament-apex-charts": "^3.0",
    "mallardduck/blade-lucide-icons": "^1.11",
    "rinvex/countries": "^9.0",
    "shipu/watchable": "dev-master",
    "symfony/polyfill-intl-icu": "^1.27"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8",
    "barryvdh/laravel-ide-helper": "^2.10",
    "fakerphp/faker": "^1.9.1",
    "larastan/larastan": "2.7",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.1",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Filament\\": "src/Filament",
      "Laravel\\": "src/Laravel",
      "Taka\\": "src/Taka"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "ide-helper": [
      "@php artisan ide-helper:eloquent",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan ide-helper:models --write-mixin"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
